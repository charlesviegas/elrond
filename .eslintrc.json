{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "browser": true,
    "amd": true,
    "jasmine": true
  },
  "globals": {
    "module": true,
    "inject": true,
    "legolas": true
  },
  "extends": "eslint:recommended"
}